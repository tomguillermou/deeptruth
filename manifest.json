{
    "manifest_version": 2,

    "name": "DeepTruth",
    "description": "Protect yourself from DeepFakes",
    "version": "0.1",

    "icons": {
        "128": "icons/icon128.png",
        "48": "icons/icon48.png",
        "16": "icons/icon16.png"
    },

    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },



    "browser_action": {
        "default_icon": "icons/icon16.png",
        "default_popup": "popup.html"
    }
}