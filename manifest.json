{
    "manifest_version": 2,
    "name": "DeepTruth",
    "description": "Protect yourself from DeepFakes",
    "version": "0.1",
    "background": {
        "scripts": [
            "scripts/background.js"
        ]
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "scripts/jquery-3.4.1.min.js",
                "scripts/content.js"
            ]
        }
    ],
    "browser_action": {
        "default_popup": "views/popup.html",
        "default_icon": "images/icon.png"
    }
}